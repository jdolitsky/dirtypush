{
    "name": "dirtypush",
    "version": "0.0.1",
    "description": "autosave css edits in chrome devtools",
    "devtools_page": "devtools.html",
    "background": {
        "page":"background.html"
    },
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "permissions": [
        "tabs",
        "http://*/"
    ],

    "manifest_version": 2,
    "minimum_chrome_version": "17"
}
